{
    "name": "tony-sol/symfony-api-client",
    "license": "MIT",
    "description": "HTTP Client for any external APIs",
    "authors": [
        {
            "name": "Tony Sol",
            "email": "tony.kent.nar.earth@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7",
        "psr/http-client": "^1",
        "psr/log": "^1|^2|^3",
        "symfony/framework-bundle": "^7",
        "symfony/serializer": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1",
        "squizlabs/php_codesniffer": "^3",
        "vimeo/psalm": "^4"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "ApiClient\\": "src/"
        }
    }
}
